script/source = "extends Node3D



func _ready() -> void:
	%TutorialEncounter.encounter_over.connect(make_evening)
	#make_evening()

func make_evening() -> void:
	var tween : Tween = get_tree().create_tween()
	tween.set_parallel()
	tween.tween_property(%Sun,\"rotation:x\",deg_to_rad(-8.9),4.0)
	tween.tween_property(%Sun,\"color\",Color(255,181,150),4.0)
	await tween.finished
	tween = get_tree().create_tween()
	tween.set_parallel()
	tween.tween_property(%Sun,\"rotation:x\",deg_to_rad(0.5),4.0)
	tween.tween_property(%Sun,\"color\",Color(255,181,150),4.0)
"

